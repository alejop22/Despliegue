<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:camel="http://camel.apache.org/schema/spring"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans https://www.springframework.org/schema/beans/spring-beans-3.0.xsd        http://camel.apache.org/schema/spring https://camel.apache.org/schema/spring/camel-spring.xsd">
	<routeContext id="route-context-crear-usuario" xmlns="http://camel.apache.org/schema/spring">
		<route id="route-crear-usuario">
            <from id="from-rest-crear" uri="direct:crear-usuario"/>
            <log id="log-inicio-crear" message="=== INICIO SERVICIO CREAR==="/>
            <setProperty id="_setPropertyAuthorization-crear" propertyName="Authorization">
                <simple>${header.Authorization} </simple>
            </setProperty>
            <removeHeaders id="_removeTodosHeaders-crear" pattern="*"/>
            <setHeader headerName="Content-Type" id="_setHeaderContentTypeCU-crear">
                <constant>application/json</constant>
            </setHeader>
            <setHeader headerName="Accept" id="_setHeaderAcceptCU-crear">
                <constant>application/json</constant>
            </setHeader>
            <setHeader headerName="Authorization" id="_setHeaderAuthorization-crear">
                <simple>${property.Authorization}</simple>
            </setHeader>
            <setHeader headerName="lotacao" id="_setHeader-crear">
                <simple>1</simple>
            </setHeader>
            <setHeader headerName="camelHttpMethod" id="_setHeaderCamelHttpMethodCC-crear">
                <constant>POST</constant>
            </setHeader>
            <marshal id="_marshalRequest-crear">
                <custom ref="jack"/>
            </marshal>
            <to id="_toHttpAU-crear" uri="https4://{{url.usuarios}}?HttpMethod=POST&amp;bridgeEndpoint=true&amp;throwExceptionOnFailure=false"/>
            <choice id="_choice-resultado-crear">
                <when id="_when-resultado-ok-crear">
                    <simple resultType="boolean">${header.CamelHttpResponseCode} == '200'</simple>
                    <unmarshal id="_unmarshalRequest-crear">
                        <custom ref="jack"/>
                    </unmarshal>
                </when>
            </choice>
        </route>
	</routeContext>
</beans>
